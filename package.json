    "highlight.js": "^11.6.0",
    "highlightjs-cobol": "^0.3.3",
    "naive-ui": "^2.33.5",
    "playwright": "^1.27.1",
    "rollup": "^3.2.4",
    "typescript": "^4.8.4",
    "vite": "^3.2.2",
    "vite-plugin-pwa": "^0.13.1",
    "vite-plugin-run": "^0.2.0",
    "vite-plugin-static-copy": "^0.11.1",
    "vue": "^3.2.41",
    "vue-router": "^4.1.6",
    "vue-tsc": "^1.0.9"
